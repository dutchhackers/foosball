{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "name": "api-core-data",
  "sourceRoot": "libs/api-core/data/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/foosball-data",
        "main": "libs/api-core/data/src/index.ts",
        "tsConfig": "libs/api-core/data/tsconfig.lib.json",
        "assets": ["libs/api-core/data/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/api-core/data/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/api-core/data"],
      "options": {
        "jestConfig": "libs/api-core/data/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
